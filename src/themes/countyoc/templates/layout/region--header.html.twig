{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{% embed "@organisms/site/site-header/site-header.twig" %}
  {% block header__navigation %}
    {% embed "@organisms/navigation/navigation.twig" %}
      {% block header__logo_top %}
      {% endblock %}
      {% block header__menu %}
        {{ elements.mainnavigation_2 }}
      {% endblock %}
      {% block header__search %}
        {{ elements.searchform }}
      {% endblock %}
    {% endembed %}
  {% endblock %}
{% endembed %}

{% block content %}
  {{ elements.countyoc_breadcrumbs }}
  {{ elements.countyoc_messages }}
{% endblock %}